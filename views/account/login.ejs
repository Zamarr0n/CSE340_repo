<% if (title) { %>
    <h1><%= title %></h1>
    <% } else {
    } %>
    <%- messages() %>
    <form id="loginForm" class=""  action="/account/login" method="post">
    <div class="formulary">
        <fieldset>
            <br>
        <label for="">Email Address:</label> <br>
        <input type="email" name="email_Address" id="Name" required>
        <br>
        <label for="password">Password: </label> <br>
        <input type="password" name="password" id="password" pattern="^(?=.*\d)(?=.*[a-z])(?=.*[A-Z])(?=.*[^a-zA-Z0-9])(?!.*\s).{12,}$" required><br>
        <p>
            Password must be minimun of 12 characters and include 
            1 Capital letter, 1 number and 1 special character. 
        </p>
    
        <input class="btn-password" type="button" value="Show Password" id="passwordBtn">
        <br>
        <input type="submit" value="Login" class="btn-login">
    
    </fieldset>
    <div class="question">
        <p>No Account? <a href="/account/register">Sign-up</a></p>
    </div>
    </div>
    </form>